package tierPSO;

import java.util.ArrayList;

public class Tier {
	private Tier parent;
	private ArrayList<Tier> children;
	private double level;
	
	public Tier(double level) {
		children = new ArrayList<Tier>();
		
	}
	public Tier(Tier parent, double level) {
		this.parent = parent;
		children = new ArrayList<Tier>();
	}
	
	public void addTier(Tier child) {
		children.add(child);
	}
	public double getLevel() {
		return level;
	}
	public void removeTier(Tier child) {
		children.remove(child);
	}
	
	
}
